{
	"presets": [
		["@babel/preset-env", {
			"exclude": [ "@babel/plugin-transform-exponentiation-operator" ]
		}]
	],
	"plugins": [
		"@babel/transform-async-to-generator",
		"@babel/plugin-proposal-export-namespace-from",
		"@babel/plugin-syntax-bigint",
		[
			"module-resolver", {
				"alias": {
					"akso": "./dist"
				}
			}
		]
	]
}
